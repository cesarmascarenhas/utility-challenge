{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./app",
    "paths": {
      "@UI": ["ui"],
      "@Components": ["components"],
      "@Controllers": ["controllers"],
      "@Types": ["types"],
      "@Helpers": ["helpers"],
      "@Assets": ["assets"],
      "@Context": ["context"],
      "@Views": ["views"],
      "@Components/*": ["components/*"],
      "@Controllers/*": ["controllers/*"],
      "@Types/*": ["types/*"],
      "@Helpers/*": ["helpers/*"],
      "@Assets/*": ["assets/*"],
      "@Context/*": ["context/*"],
      "@Views/*": ["views/*"]
    }
  }
}
